{
  "name": "website-museu-jacinto-sousa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "em:start": "firebase emulators:start --import ./emulator_data",
    "em:dev": "firebase emulators:exec --import ./emulator_data  \"npm run dev\"",
    "test": "npx concurrently \"npm run em:start\" \"npm run cypress:open\""
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.2",
    "@mui/material": "^6.0.1",
    "@mui/x-date-pickers": "^7.19.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "firebase": "^10.14.0",
    "mui-tel-input": "^6.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-iframe": "^1.8.5",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^6.26.2",
    "vite-bundle-visualizer": "^1.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.3.2",
    "cypress": "^13.15.0",
    "cypress-firebase": "^4.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "typescript": "^5.6.2",
    "vite": "^4.5.3"
  }
}
